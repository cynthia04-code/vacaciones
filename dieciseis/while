public class dieciseis{
  public static void main(String arg[]){
    int a = 0;
    int b = 1;
    int c = 0;
    int i = 0;
      while(i<10){
        if(i < 9){
          System.out.print(a + ", ");
          c = a + b;
          a = b;
          b = c;
          }
            else{
            System.out.print(a);
            }
      i++;
      }
  }    
}


